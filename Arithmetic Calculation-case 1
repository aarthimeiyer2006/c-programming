#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a,b;
    char op;
    float add,sub,mul,div,mod;
    scanf("%d\n",&a);
    scanf("%d\n",&b);
    scanf("%c\n",&op);
    switch(op)
    {
    case '+': 
            add=a+b;
    printf("Addition of two number is %.1f\n",add);
    break;
    case '-':
            sub=a-b;
    printf("Subtraction of two number is %.1f\n",sub);
    break;
    case '*':
            mul=a*b;
    printf("Multiplication of two number is %.1f\n",mul);
    break;
    case '/':
            div=a/b;
    printf("Division of two number is %.1f\n",div);
    break;
        case '%':
            mod=a%b;
    printf("Modulo division of two number is %.1f",mod);
            break;
    default:
    printf("Invalid Input");
    }
    return 0;
}
